{
  "name": "Magic",
  "version": "1.0",
  "description": "Does magical stuff",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "contextMenus",
    "*://*.serpapi.com/*",
    "*://*.quizlet.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "325": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "325": "icon.png"
    }
  },
  "web_accessible_resources": [
    "mouseover.html"
  ],
  "manifest_version": 2
}
